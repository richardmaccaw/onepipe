{
  "name": "onepipe",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "check:types": "tsc --noEmit",
    "build": "pnpm -r run build",
    "build:client": "pnpm --filter @onepipe/client run build",
    "build:core": "pnpm --filter @onepipe/core run build",
    "build:worker": "pnpm --filter @onepipe/worker run build",
    "build:destination-bigquery": "pnpm --filter @onepipe/destination-bigquery run build",
    "dev:client": "pnpm --filter @onepipe/client run dev",
    "dev:worker": "pnpm --filter @onepipe/worker run dev"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250603.0",
    "@types/lodash": "^4.17.16",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.8.3",
    "wrangler": "^4.13.2"
  },
  "dependencies": {
    "@maccman/web-auth-library": "^1.0.5",
    "cloudflare-basics": "^0.0.9",
    "lodash": "^4.17.21",
    "uuid": "^11.1.0",
    "web-auth-library": "^1.0.3",
    "zod": "^3.24.2"
  }
}
